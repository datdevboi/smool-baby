type User {
  id: ID! @unique
  email: String! @unique
  password: String!
  babies: [Baby!]!
}

enum DiaperType {
  Mixed
  Pee
  Poop
}

type Diaper {
  id: ID! @unique
  type: DiaperType!
  time: DateTime!
  baby: Baby!
}

type Baby {
  id: ID! @unique
  name: String!
  dob: DateTime!
  parent: User!
  pictureUrl: String
  diapers: [Diaper!]
}
